{
  "compilerOptions": {
    "target": "es5",
    "module": "NodeNext",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
//    "moduleResolution": "bundler"

  },
  "exclude": ["node_modules"],
  "include": ["src/**/*"]
}